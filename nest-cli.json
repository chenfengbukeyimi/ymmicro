{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/ymmicro/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/ymmicro/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/ymmicro",
  "projects": {
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "micro-auth": {
      "type": "application",
      "root": "apps/micro-auth",
      "entryFile": "main",
      "sourceRoot": "apps/micro-auth/src",
      "compilerOptions": {
        "tsConfigPath": "apps/micro-auth/tsconfig.app.json"
      }
    },
    "micro-config": {
      "type": "application",
      "root": "apps/micro-config",
      "entryFile": "main",
      "sourceRoot": "apps/micro-config/src",
      "compilerOptions": {
        "tsConfigPath": "apps/micro-config/tsconfig.app.json"
      }
    },
    "micro-log": {
      "type": "application",
      "root": "apps/micro-log",
      "entryFile": "main",
      "sourceRoot": "apps/micro-log/src",
      "compilerOptions": {
        "tsConfigPath": "apps/micro-log/tsconfig.app.json"
      }
    },
    "micro-user": {
      "type": "application",
      "root": "apps/micro-user",
      "entryFile": "main",
      "sourceRoot": "apps/micro-user/src",
      "compilerOptions": {
        "tsConfigPath": "apps/micro-user/tsconfig.app.json"
      }
    },
    "ymmicro": {
      "type": "application",
      "root": "apps/ymmicro",
      "entryFile": "main",
      "sourceRoot": "apps/ymmicro/src",
      "compilerOptions": {
        "tsConfigPath": "apps/ymmicro/tsconfig.app.json"
      }
    }
  }
}